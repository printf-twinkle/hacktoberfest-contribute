import java.util.*;
public class palindrom
{
    public static void main()
    {
        Scanner sc= new Scanner(System.in);
        System.out.println("Enter a number");
        int n=sc.nextInt();
        int i=0,d=0,r=0;
        for(i=n; i>0; i=i/10)
        {
            d=i%10;
            r=r*10+d;
        }
        if(r==n)
        System.out.println("Palindrom number");
        else 
        System.out.println("Not a palindrom number");
    }
}
