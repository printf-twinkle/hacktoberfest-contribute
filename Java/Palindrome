import java.util.*;
class palindrome
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("Please enter a no.");
        int num=sc.nextInt();
        int temp=num,digit,sum=0;
        
        while(num>0)
        {
            digit=num%10;
            sum=sum*10+digit;
            num=num/10;
        }
        if(temp==sum)
        System.out.println("Palindrome no.");
        else
        System.out.println("Not a palindrome no.");
    }
}
