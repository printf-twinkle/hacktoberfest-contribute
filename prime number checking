import java.util.*;
public class prime
{
    public static void main()
    {
        Scanner sc= new Scanner(System.in);
        System.out.println("Enter a number");
        int n=sc.nextInt();
        int i=0,j=0;
        for(i=1;i<=n;i++)
        {
            if(n%i==0)
            j++;
        }
        if(j==2)
        System.out.println("Prime number");
        else
        System.out.println("Not a prime number");
    }
} 
