import java.util.*;
public class prime
{
public static void main(String args[])
{
	int i, n, c=0;
	Scanner sc= new Scanner(System.in);
	n=sc.nextInt();
	
	for(i=0; i<n ;i++)
	{
		if(n%i==0)
		c++;
	}
	
	if(c==2)
	System.out.println(n + " is a prime number");
	else
	System.out.println(n + " is a prime number");
	}
}
