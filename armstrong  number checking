import java.util.*;
public class armstrong
{
    public static void main()
    {
        Scanner sc= new Scanner(System.in);
        System.out.println("Enter a number");
        int n=sc.nextInt();
        int i=0,d=0,r=0;
        for(i=n; i>0; i=i/10)
        {
            d=i%10;
            r=(d*d*d)+r;
        }
        if(r==n)
        System.out.println("Armstrong number");
        else 
        System.out.println("Not a Armstrong number");
    }
}
