import java.util.*;
public class factorial
{
    public static void main()
    {
        Scanner sc= new Scanner(System.in);
        System.out.println("Enter a number");
        int n=sc.nextInt();
        int i=0,f=1;
        for(i=1;i<=n;i++)
        {
            f=f*i;
        }
        System.out.println("Factorial of the number= "+f);
    }
}
